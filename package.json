{
  "name": "express-server-part-ii",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=testLocal NODE_PATH=./ nyc mocha --require @babel/register --verbose --serial",
    "test-watch": "export WATCH='watch' && nodemon --exec 'npm test'",
    "start": "NODE_ENV=dev NODE_PATH=./ nodemon --exec babel-node server/index.js"
    "delete-dist": "rm -rf dist",
    "transcompile": "babel . -d dist --ignore node_modules --source-maps",
    "copy": "cp package.json dist/package.json",
    "install-prod": "cd dist && npm i --production",
    "build": "npm run-script delete-dist && npm run-script transcompile && npm run-script copy && npm run-script install-prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "mongodb": "^3.2.7",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/types": "^7.4.4",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/mongodb": "^3.1.27",
    "@types/node": "^12.0.7",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "fs-extra": "^8.0.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
